{
  "name": "ng-simbayu",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js --port 4600",
    "start:o": "npm run env -s && ng serve --proxy-config proxy.conf.js --port 4600 --open",
    "start:prod": "npm run env -s && ng serve --prod --proxy-config proxy.conf.js --port 4600",
    "start:prodo": "npm run env -s && ng serve --prod --proxy-config proxy.conf.js --port 4600 --open",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^8.1.0",
    "@angular/common": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/forms": "^8.1.0",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "@angular/service-worker": "^8.1.0",
    "@aspnet/signalr": "^1.1.4",
    "@ng-bootstrap/ng-bootstrap": "^5.0.0-rc.1",
    "@ngx-translate/core": "^11.0.1",
    "@types/crypto-js": "^3.1.43",
    "@types/echarts": "^4.4.2",
    "angular-font-awesome": "^3.1.2",
    "angular-user-idle": "^2.2.1",
    "bootstrap": "^4.1.1",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.2.0-rc.2",
    "echarts-gl": "^1.1.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "highcharts": "^9.0.0",
    "leaflet": "^1.6.0",
    "moment": "^2.24.0",
    "ng5-slider": "^1.2.4",
    "ngx-chips": "2.0.2",
    "ngx-echarts": "^4.1.0",
    "ngx-gallery": "^5.10.0",
    "ngx-infinite-scroll": "^8.0.0",
    "ngx-ui-switch": "8.3.0",
    "rxjs": "^6.5.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.0.4",
    "@angular-devkit/build-angular": "^0.801.0",
    "@angular/cli": "~8.1.0",
    "@angular/compiler-cli": "^8.1.0",
    "@angular/language-service": "^8.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@asymmetrik/ngx-leaflet": "^5.0.2",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^4.0.0",
    "@syncfusion/ej2-angular-richtexteditor": "^16.4.55",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.3",
    "@types/jest": "^24.0.12",
    "@types/leaflet": "^1.5.8",
    "@types/node": "^10.9.0",
    "@wishtack/reactive-component-loader": "^1.1.1",
    "@wishtack/rx-scavenger": "^2.0.1",
    "codelyzer": "^5.1.0",
    "echarts": "^4.2.0-rc.2",
    "echarts-gl": "^1.1.1",
    "hads": "^1.7.0",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^2.0.0",
    "husky": "^3.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.7.1",
    "jest-preset-angular": "^7.1.0",
    "ng-multiselect-dropdown": "0.2.4",
    "ngx-echarts": "^4.1.0",
    "ngx-gallery": "^5.10.0",
    "ngx-toastr": "^9.2.0",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.17.0",
    "stylelint": "~10.1.0",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended-scss": "~3.3.0",
    "stylelint-config-standard": "~18.3.0",
    "stylelint-scss": "~3.9.0",
    "ts-node": "^8.3.0",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.4.0"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
